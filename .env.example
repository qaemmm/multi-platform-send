# 数据库配置
# 开发环境使用本地SQLite
DATABASE_URL="file:./dev.db"

# 生产环境使用Turso
TURSO_DATABASE_URL="libsql://your-database-name.turso.io"
TURSO_AUTH_TOKEN="your-turso-auth-token"

# NextAuth.js 认证配置
NEXTAUTH_SECRET="your-super-secret-key-at-least-32-characters-long"
NEXTAUTH_URL="http://localhost:3000"

# 应用配置
NEXT_PUBLIC_APP_URL="http://localhost:3000"
NEXT_PUBLIC_API_BASE_URL="http://localhost:3000"

# 云存储配置 (Cloudflare R2)
R2_ACCOUNT_ID="your-cloudflare-account-id"
R2_ACCESS_KEY_ID="your-r2-access-key-id"
R2_SECRET_ACCESS_KEY="your-r2-secret-access-key"
R2_BUCKET_NAME="your-bucket-name"
R2_PUBLIC_URL="https://your-domain.com"

# 七牛云存储配置
QINIU_ACCESS_KEY="your-qiniu-access-key"
QINIU_SECRET_KEY="your-qiniu-secret-key"
QINIU_BUCKET="your-qiniu-bucket-name"
QINIU_DOMAIN="https://your-qiniu-domain.com"
QINIU_ZONE="Zone_z0"  # 可选值: Zone_z0(华东), Zone_z1(华北), Zone_z2(华南), Zone_na0(北美), Zone_as0(东南亚)

# 图片存储提供商选择 (r2 | qiniu | auto)
# auto: 优先使用R2，失败时回退到七牛云
IMAGE_STORAGE_PROVIDER="auto"

# 可选：AI服务配置
OPENAI_API_KEY="your-openai-api-key"
OPENAI_BASE_URL="https://api.openai.com/v1"

# 可选：分析服务
GOOGLE_ANALYTICS_ID="G-XXXXXXXXXX"
UMAMI_WEBSITE_ID="your-umami-website-id"

# 可选：支付服务
STRIPE_PUBLIC_KEY="pk_test_..."
STRIPE_SECRET_KEY="sk_test_..."
STRIPE_WEBHOOK_SECRET="whsec_..."

# 可选：邮件服务
RESEND_API_KEY="re_..."
FROM_EMAIL="noreply@ziliu.online"

# 开发环境标识
NODE_ENV="development"

